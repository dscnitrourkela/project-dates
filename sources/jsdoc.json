{
    "source":{
        "include":["."],
        "includePattern":".+\\.js(doc|x)?$",
        "excludePattern":"node_modules"
    },
    "plugins":["plugins/markdown"],
    "templates":{
        "cleverLinks":true,
        "monospaceLinks":true
    },
    "opts":{
        "recurse":true,
        "destination":"../docs/"
    }
}